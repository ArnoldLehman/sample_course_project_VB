<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <configSections>
        <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" >
            <section name="SampleCourseProject.My.MySettings" type="System.Configuration.ClientSettingsSection, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false" />
        </sectionGroup>
    </configSections>
    <connectionStrings>
        <add name="SampleCourseProject.My.MySettings.BookkeepingDatabaseConnectionString"
            connectionString="Provider=Microsoft.Jet.OLEDB.4.0;Data Source=&quot;C:\Users\Дом\Documents\Database\Курсовой проект\BookkeepingDatabase.mdb&quot;"
            providerName="System.Data.OleDb" />
        <add name="SampleCourseProject.My.MySettings.BookkeepingDatabaseConnectionString1"
            connectionString="Provider=Microsoft.Jet.OLEDB.4.0;Data Source=|DataDirectory|\BookkeepingDatabase.mdb"
            providerName="System.Data.OleDb" />
        <add name="SampleCourseProject.My.MySettings.AccountPersonalConnectionString"
            connectionString="Provider=Microsoft.Jet.OLEDB.4.0;Data Source=|DataDirectory|\AccountPersonal.mdb"
            providerName="System.Data.OleDb" />
        <add name="SampleCourseProject.My.MySettings.RegistrationStaffConnectionString"
            connectionString="Provider=Microsoft.Jet.OLEDB.4.0;Data Source=|DataDirectory|\RegistrationStaff.mdb"
            providerName="System.Data.OleDb" />
        <add name="SampleCourseProject.My.MySettings.UserRegistrationConnectionString"
            connectionString="Provider=Microsoft.Jet.OLEDB.4.0;Data Source=|DataDirectory|\UserRegistration.mdb"
            providerName="System.Data.OleDb" />
        <add name="SampleCourseProject.My.MySettings.UserAccountConnectionString"
            connectionString="Provider=Microsoft.Jet.OLEDB.4.0;Data Source=|DataDirectory|\UserAccount.mdb"
            providerName="System.Data.OleDb" />
    </connectionStrings>
    <system.diagnostics>
        <sources>
            <!-- В этой секции определяется конфигурация ведения журнала для My.Application.Log -->
            <source name="DefaultSource" switchName="DefaultSwitch">
                <listeners>
                    <add name="FileLog"/>
                    <!-- Раскомментируйте следующую секцию для записи в журнал событий приложения -->
                    <!--<add name="EventLog"/>-->
                </listeners>
            </source>
        </sources>
        <switches>
            <add name="DefaultSwitch" value="Information" />
        </switches>
        <sharedListeners>
            <add name="FileLog"
                 type="Microsoft.VisualBasic.Logging.FileLogTraceListener, Microsoft.VisualBasic, Version=8.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a, processorArchitecture=MSIL"
                 initializeData="FileLogWriter"/>
            <!-- Раскомментируйте следующую секцию и замените APPLICATION_NAME на имя своего приложения для записи в журнал событий приложения -->
            <!--<add name="EventLog" type="System.Diagnostics.EventLogTraceListener" initializeData="APPLICATION_NAME"/> -->
        </sharedListeners>
    </system.diagnostics>
    <userSettings>
        <SampleCourseProject.My.MySettings>
            <setting name="LoginUser" serializeAs="String">
                <value>user</value>
            </setting>
            <setting name="PasswordUser" serializeAs="String">
                <value>passuser</value>
            </setting>
            <setting name="LoginAdmin" serializeAs="String">
                <value>admin</value>
            </setting>
            <setting name="PasswordAdmin" serializeAs="String">
                <value>passadmin</value>
            </setting>
        </SampleCourseProject.My.MySettings>
    </userSettings>
</configuration>
